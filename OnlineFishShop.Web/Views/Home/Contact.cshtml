@{
    ViewData["Title"] = OnlineFishShopResources.lblContatcs;
}
@using OnlineFishShop.Common.Resources
<h2 class="page-title">@ViewData["Title"]</h2>
@*<address>
        One Microsoft Way<br />
        Redmond, WA 98052-6399<br />
        <abbr title="Phone">P:</abbr>
        425.555.0100
    </address>

    <address>
